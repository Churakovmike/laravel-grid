{
    "name": "churakovmike/laravel-grid",
    "type": "library",
    "description": "Easy gridview for laravel",
    "keywords": [
        "laravel",
        "grid",
        "gridview",
        "table"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Churakov Mike"
        },
        {
            "name": "Churakova Elizaveta"
        }
    ],
    "require": {
        "illuminate/view": "^5.5|^7",
        "illuminate/support": "^5.5|^7",
        "illuminate/console": "^5.5|^7",
        "illuminate/contracts": "^5.5|^7",
        "illuminate/http": "^5.5|^7",
        "illuminate/database": "^5.5|^7",
        "illuminate/pagination": "^5.5|^7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.3",
        "orchestra/testbench": ">=3.0",
        "phpunit/php-code-coverage": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\EasyGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChurakovMike\\EasyGrid\\GridViewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "run:unit": [
            "phpunit ./tests"
        ]
    }
}
