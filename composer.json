{
    "name": "churakovmike/laravel-grid",
    "type": "library",
    "description": "Easy gridview for laravel",
    "keywords": [
        "laravel",
        "grid",
        "gridview",
        "table"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Churakov Mike"
        },
        {
            "name": "Churakova Elizaveta"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0@dev",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\EasyGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChurakovMike\\EasyGrid\\GridViewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "run:unit": [
            "phpunit ./tests"
        ]
    }
}
